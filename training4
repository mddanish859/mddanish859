#include <stdio.h>

int main()
{
    int radius;
    float pi = 3.14;
    printf("Enter the value of radius \n");
    scanf("%d",&radius);
    printf("Area of the circle is %f %d \n",pi*radius*radius);
    int height;
    printf("Enter the value of height \n");
    scanf("%d",&height);
    printf("Enter the vlalue of radius \n");
    scanf("%d",&radius);
    printf("Volume of the cyclinder is  %d %f \n",pi*radius*radius*height);

    return 0;
}
